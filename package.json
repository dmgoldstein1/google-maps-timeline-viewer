{
  "name": "google-maps-timeline-viewer",
  "version": "2.0.0",
  "description": "Self-hosted Google Maps Timeline viewer with offline caching and Docker support",
  "main": "server/api.js",
  "type": "module",
  "scripts": {
    "start": "node server/api.js",
    "dev": "NODE_ENV=development node --watch server/api.js",
    "prefetch": "node scripts/prefetch-places.js",
    "lint": "echo 'No linter configured' && exit 0",
    "test": "node --test tests/prefetch-sse.test.js tests/server.test.js",
    "docker:build": "docker build -t google-maps-timeline-viewer .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "google-maps",
    "timeline",
    "location-history",
    "docker",
    "offline",
    "caching"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "p-queue": "^7.4.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
